{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["warn"],
    "indent": ["warn", 2],
    "quotes": ["warn", "double"],
    "semi": ["error", "always"],
    "no-unused-vars": ["warn"],
    "camelcase": ["warn"]
  }
}
