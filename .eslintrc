{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["warn", "double"],
    "no-unused-vars": ["off"],
    "linebreak-style": ["off"]
    // "no-console": ["warn"],
    // "indent": ["warn", 2],
    // "semi": ["error", "always"],
    // "camelcase": ["warn"]
  },
  "extends": ["airbnb-base"]
}
